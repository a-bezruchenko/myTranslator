{
    "@программа@":{
        "MODULE": ["MODULE", "id", ";", "@блок объявлений@", "BEGIN", "@тело@",
                "END", "id", "."]
    },
    "@блок объявлений@":{
        "VAR":["VAR", "id","@последовательность id@", ":", "@тип@",";","@объявление переменных@", "@блок объявлений@"],
        "TYPE":["TYPE","id", "=", "@тип@",";", "@объявление типа@", "@блок объявлений@"],
        "PROCEDURE": ["PROCEDURE","id","(", "@параметры процедуры@",")",
             "@возвращаемое значение процедуры@", "@блок объявлений@","BEGIN",
             "@тело@","END","id",";"],
        "BEGIN": []
    },
    "@объявление переменных@": {
        "id": ["id","@последовательность id@", ":", "@тип@",";","@объявление переменных@"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE","PROCEDURE"]
    },
    "@последовательность id@":{
        ",": [",", "id", "@последовательность id@"],
        ":": []
    },
    "@объявление типа@": {
        "id": ["id", "=", "@тип@", ";"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE", "PROCEDURE"]
    },
    "@возвращаемое значение процедуры@": {
        ":": [":","@тип@"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE", "PROCEDURE"]
    },
    "@параметры процедуры@": {
        "id": ["id", ":", "@тип@", "@параметры процедуры через запятую@"],
        ")": []
    },
    "@параметры процедуры через запятую@":{
        ",": [",", "id", ":", "@тип@", "@параметры процедуры через запятую@"],
        ")": []
    },
    "@тип@": {
        "INTEGER" : ["INTEGER"],
        "REAL" : ["REAL"],
        "BOOLEAN" : ["BOOLEAN"],
        "CHAR" : ["CHAR"],
        "STRING" : ["STRING"],
        "ARRAY" : ["ARRAY", "OF", "@тип@"]
    },
    "@тело@": {
        ";": [";", "@тело@"],
        "const": ["@простое выражение@", ";", "@тело@"],
        "id": ["id","@действие с id@",";", "@тело@"],
        "IF": ["IF", "@выражение@", "THEN", "@тело@", "@блоки elsif@", "@блок else@" ,"END", "@тело@"],
        "WHILE": ["WHILE", "@выражение@", "DO", "@тело@","END", "@тело@"],
        "CASE": ["CASE", "@выражение@", "OF", "@вариант@", "@варианты@", "@блок else@", "END", "@тело@"],
        "REPEAT": ["REPEAT", "@тело@", "UNTIL", "@выражение@", "@тело@"],
        "END": [],
        "DEC": ["DEC", "@значение@", ";"],
        "INC": ["INC", "@значение@", ";"]
    },
    "@действие с id@": {
        ":=": [":=", "@выражение@"],
        "(": ["(","@аргументы вызываемой функции@", ")"],
        ";": []
    },
    "@аргументы вызываемой функции@": { 
        ")": [], 
        "id": ["id", "@аргументы вызываемой функции с запятой@"],
        "const" : ["const", "@аргументы вызываемой функции с запятой@"]
    },
    "@аргументы вызываемой функции с запятой@": {
        "," : [",", "@значение@", "@аргументы вызываемой функции с запятой@"],
        ")" : []
    },
    "@значение@" : {
        "id" : ["id"],
        "const" : ["const"]
    },
    "@выражение@": {
        "id": ["@простое выражение@", "@сравнение с пр. выражением@"],
        "const": ["@простое выражение@", "@сравнение с пр. выражением@"]
    },
    "@сравнение с пр. выражением@": { 
        ">" :[">",  "@простое выражение@"], 
        "<" :["<",  "@простое выражение@"], 
        "=":["=",   "@простое выражение@"], 
        ">=":[">=", "@простое выражение@"], 
        "<=":["<=", "@простое выражение@"], 
        "#":["#", "@простое выражение@"], 
        ";":[] 
    }, 
    "@простое выражение@": { 
        "+": ["@последовательность слагаемых@"], 
        "-": ["@последовательность слагаемых@"], 
        "(": ["(","@простое выражение@",")"], 
        "id": ["@слагаемое@", "@последовательность слагаемых@"],
        "const" : ["@слагаемое@", "@последовательность слагаемых@"] 
    }, 
    "@последовательность слагаемых@": { 
        "+": ["+", "@слагаемое@", "@последовательность слагаемых@"], 
        "-": ["-", "@слагаемое@", "@последовательность слагаемых@"], 
        "OR": ["OR", "@слагаемое@", "@последовательность слагаемых@"],
        "EMPTY PRODUCTIONS": [")", ";", "<", ">","=", "#", ">=", "<=", "DO", "THEN"]
    }, 
    "@слагаемое@": { 
        "(": ["(","@простое выражение@",")", "@последовательность множителей@"], 
        "id": ["@множитель@", "@последовательность множителей@"],
        "const": ["@множитель@", "@последовательность множителей@"] 
    }, 
    "@последовательность множителей@": { 
        "*": ["*", "@множитель@", "@последовательность множителей@"], 
        "/": ["/", "@множитель@", "@последовательность множителей@"], 
        "DIV": ["DIV", "@множитель@", "@последовательность множителей@"], 
        "MOD": ["MOD", "@множитель@", "@последовательность множителей@"], 
        "&": ["&", "@множитель@", "@последовательность множителей@"],
        "EMPTY PRODUCTIONS": ["+", "-", "OR", ";", ")", "<", ">","=", "#", ">=", "<=", "DO", "THEN"]
    }, 
    "@множитель@": { 
        "~": ["~","@выражение@"], 
        "id": ["id"],
        "const": ["const"]
    }
}