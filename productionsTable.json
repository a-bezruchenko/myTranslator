{
    "@программа@":{
        "MODULE": ["MODULE", "id", ";", "@блок об.@", "BEGIN", "@тело@",
                "END", "id", "."]
    },
    "@блок об.@":{
        "VAR":["VAR", "id","@посл. id@", ":", "@тип@",";","@объявл. пер.@", "@блок об.@"],
        "TYPE":["TYPE","id", "=", "@тип@",";", "@объявл. типа@", "@блок об.@"],
        "PROCEDURE": ["PROCEDURE","id","(", "@пар. проц.@",")",
             "@возвр. знач. проц.@", "@блок об.@","BEGIN",
             "@тело@","END","id",";"],
        "BEGIN": []
    },
    "@объявл. пер.@": {
        "id": ["id","@посл. id@", ":", "@тип@",";","@объявл. пер.@"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE","PROCEDURE"]
    },
    "@посл. id@":{
        ",": [",", "id", "@посл. id@"],
        ":": []
    },
    "@объявл. типа@": {
        "id": ["id", "=", "@тип@", ";"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE", "PROCEDURE"]
    },
    "@возвр. знач. проц.@": {
        ":": [":","@тип@"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE", "PROCEDURE"]
    },
    "@пар. проц.@": {
        "id": ["id", ":", "@тип@", "@пар.проц. ч.з.@"],
        ")": []
    },
    "@пар.проц. ч.з.@":{
        ",": [",", "id", ":", "@тип@", "@пар.проц. ч.з.@"],
        ")": []
    },
    "@тип@": {
        "INTEGER" : ["INTEGER"],
        "REAL" : ["REAL"],
        "BOOLEAN" : ["BOOLEAN"],
        "CHAR" : ["CHAR"],
        "STRING" : ["STRING"],
        "ARRAY" : ["ARRAY", "OF", "@тип@"]
    },
    "@тело@": {
        ";": [";", "@тело@"],
        "const": ["@пр. выр.@", ";", "@тело@"],
        "id": ["id","@действие с id@",";", "@тело@"],
        "IF": ["IF", "@выражение@", "THEN", "@тело@", "@блоки elsif@", "@блок else@" ,"END", "@тело@"],
        "WHILE": ["WHILE", "@выражение@", "DO", "@тело@","END", "@тело@"],
        "CASE": ["CASE", "@выражение@", "OF", "@вариант@", "@варианты@", "@блок else@", "END", "@тело@"],
        "REPEAT": ["REPEAT", "@тело@", "UNTIL", "@выражение@", "@тело@"],
        "END": [],
        "DEC": ["DEC", "@значение@", ";"],
        "INC": ["INC", "@значение@", ";"]
    },
    "@действие с id@": {
        ":=": [":=", "@выражение@"],
        "(": ["(","@аргументы вызываемой функции@", ")"],
        ";": []
    },
    "@аргументы вызываемой функции@": { 
        ")": [], 
        "id": ["@выражение@", "@аргументы вызываемой функции с запятой@"],
        "const" : ["@выражение@", "@аргументы вызываемой функции с запятой@"]
    },
    "@аргументы вызываемой функции с запятой@": {
        "," : [",", "@выражение@", "@аргументы вызываемой функции с запятой@"],
        ")" : []
    },
    "@значение@" : {
        "id" : ["id"],
        "const" : ["const"]
    },
    "@выражение@": {
        "id": ["@пр. выр.@", "@сравн. с пр. выр.@"],
        "const": ["@пр. выр.@", "@сравн. с пр. выр.@"]
    },
    "@сравн. с пр. выр.@": { 
        ">" :[">",  "@пр. выр.@"], 
        "<" :["<",  "@пр. выр.@"], 
        "=":["=",   "@пр. выр.@"], 
        ">=":[">=", "@пр. выр.@"], 
        "<=":["<=", "@пр. выр.@"], 
        "#":["#", "@пр. выр.@"],
        "EMPTY PRODUCTIONS": [";", ")"] 
    }, 
    "@пр. выр.@": { 
        "+": ["@последовательность слагаемых@"], 
        "-": ["@последовательность слагаемых@"], 
        "(": ["(","@пр. выр.@",")"], 
        "id": ["@слагаемое@", "@последовательность слагаемых@"],
        "const" : ["@слагаемое@", "@последовательность слагаемых@"] 
    }, 
    "@последовательность слагаемых@": { 
        "+": ["+", "@слагаемое@", "@последовательность слагаемых@"], 
        "-": ["-", "@слагаемое@", "@последовательность слагаемых@"], 
        "OR": ["OR", "@слагаемое@", "@последовательность слагаемых@"],
        "EMPTY PRODUCTIONS": [")", ";", "<", ">","=", "#", ">=", "<=", "DO", "THEN"]
    }, 
    "@слагаемое@": { 
        "(": ["(","@пр. выр.@",")", "@последовательность множителей@"], 
        "id": ["@множитель@", "@последовательность множителей@"],
        "const": ["@множитель@", "@последовательность множителей@"] 
    }, 
    "@последовательность множителей@": { 
        "*": ["*", "@множитель@", "@последовательность множителей@"], 
        "/": ["/", "@множитель@", "@последовательность множителей@"], 
        "DIV": ["DIV", "@множитель@", "@последовательность множителей@"], 
        "MOD": ["MOD", "@множитель@", "@последовательность множителей@"], 
        "&": ["&", "@множитель@", "@последовательность множителей@"],
        "EMPTY PRODUCTIONS": ["+", "-", "OR", ";", ")", "<", ">","=", "#", ">=", "<=", "DO", "THEN"]
    }, 
    "@множитель@": { 
        "~": ["~","@выражение@"], 
        "id": ["id", "@аргументы для id@"],
        "const": ["const"]
    },
    "@аргументы для id@":{
        "(": ["(","@аргументы вызываемой функции@",")"],
        "EMPTY PRODUCTIONS": ["+","-", ">", "<", ">=", "#", ";", "<=", "DIV", "MOD", "&", "OR", "*", "/", ")"]
    }
}