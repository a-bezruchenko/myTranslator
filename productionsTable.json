{
    "@программа@":{
        "MODULE": ["MODULE", "$function", "$id", "id", "$(", "$)", ";", "$\n","${", "$\n", "@блок объявлений@", "BEGIN", "@тело@",
                "END", "id", ".", "$\n", "$}"]
    },
    "@блок объявлений@":{
        "VAR":["VAR", "$var","$id","$;", "id",  "@посл. id@", ":", "@тип@",";","@объявление переменной@", "$;", "$\n", "@блок объявлений@"],
        "TYPE":["TYPE","id", "=", "@тип@",";", "@объявление типа@", "@блок объявлений@"],
        "PROCEDURE": ["PROCEDURE", "$function","$id","id", "(", "$(", "@параметры процедуры@",")","$)",
             "@возвращаемое значение процедуры@", "${","$\n", "@блок объявлений@","BEGIN",
             "@тело@","END","id","$\n","$}",";", "$\n"],
        "BEGIN": []
    },
    "@объявление переменной@": {
        "id": ["$var","$id","id", "@посл. id@", ":", "@тип@",";","@объявление переменной@"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE","PROCEDURE"]
    },
    "@посл. id@":{
        ",": [",", "$var","$id", "id", "$;", "@посл. id@"],
        ":": []
    },
    "@объявление типа@": {
        "id": ["id", "=", "@тип@", ";"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE", "PROCEDURE"]
    },
    "@возвращаемое значение процедуры@": {
        ":": [":","@тип@"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE", "PROCEDURE"]
    },
    "@параметры процедуры@": {
        "id": ["$id","id",  ":", "@тип@", "@остальные пар-ры проц-ры@"],
        ")": []
    },
    "@остальные пар-ры проц-ры@":{
        ",": [",", "$,", "$id", "id", ":", "@тип@", "@остальные пар-ры проц-ры@"],
        ")": []
    },
    "@тип@": {
        "INTEGER" : ["INTEGER"],
        "REAL" : ["REAL"],
        "BOOLEAN" : ["BOOLEAN"],
        "CHAR" : ["CHAR"],
        "STRING" : ["STRING"],
        "ARRAY" : ["ARRAY", "OF", "@тип@"]
    },
    "@тело@": {
        ";": [";", "$;","$\n", "@тело@"],
        "const": ["@оператор@","$\n", "@тело@"],
        "id": ["@оператор@","$\n", "@тело@"],
        "IF": ["IF","$if", "@выражение@", "THEN", "${", "$\n","@тело@","$}","$\n", "@блоки elsif@", "@блок else@" ,"END", "@тело@"],
        "WHILE": ["WHILE", "$while","$(", "@выражение@", "$)","DO","${","$\n", "@тело@","END","$}", "@тело@"],
        "CASE": ["CASE", "@выражение@", "OF", "@вариант@", "@варианты@", "@блок else@", "END", "@тело@"],
        "REPEAT": ["REPEAT", "$do", "${", "$\n", "@тело@","$}", "UNTIL", "$while","$(","$!", "@выражение@","$)","$\n", "@тело@"],
        "DEC": ["DEC", "@значение@", "$--",";","$;", "@тело@"],
        "INC": ["INC", "@значение@", "$++",";","$;", "@тело@"],
        "RETURN": ["RETURN", "$return", "@выражение@", "$;", "@тело@"],
        "EMPTY PRODUCTIONS" : ["END", "UNTIL", "ELSIF", "ELSE", "|"]
    },
    "@значение@" : {
        "id" : ["$id","id"],
        "const" : ["$const","const"]
    },
    "@блоки elsif@":{
        "ELSIF": ["ELSIF", "$else", "$if", "@выражение@", "THEN", "${","$\n", "@тело@","$}", "$\n","@блоки elsif@"],
        "EMPTY PRODUCTIONS": ["ELSE", "END"]
    },
    "@блок else@": {
        "ELSE": ["ELSE", "$else", "${","$\n","@тело@", "$}", "$\n"],
        "END" : []
    },
    "@вариант@" : {
        "const": ["const", "@метки варианта@", ":", "@тело@"],
        "EMPTY PRODUCTIONS" : ["END"]
    },
    "@варианты@" : {
        "|": ["|", "@вариант@"],
        "EMPTY PRODUCTIONS" : ["END"]
    },
    "@метки варианта@" : {
        ".." : ["..", "const"],
        "EMPTY PRODUCTIONS" : [":"]
    },
    "@оператор@": {
        "const": ["@пр. выражение@","$;" ,";"],
        "id": ["$id","id", "@действие с id@","$;",";"]
    },
    "@действие с id@": {
        ":=": [":=", "$=", "@выражение@"],
        "(": ["(", "$(","@аргументы вызываемой функции@", ")", "$)","@часть выражения@"],
        ";": []
    },
    "@аргументы вызываемой функции@": { 
        ")": [], 
        "id": ["@выражение@", "@аргументы вызываемой функции с запятой@"],
        "const" : ["@выражение@", "@аргументы вызываемой функции с запятой@"]
    },
    "@аргументы вызываемой функции с запятой@": {
        "," : [",","$,", "@выражение@", "@аргументы вызываемой функции с запятой@"],
        ")" : []
    },
    "@аргументы для id@":{
        "(": ["(", "$(","@аргументы вызываемой функции@",")", "$)"],
        "EMPTY PRODUCTIONS": ["+","-", ">", "<", ">=", "#", ";", "<=", "DIV", "MOD", "&", "OR", "*", "/", ")", "OF"]
    },
    "@выражение@": {
        "(": ["(", "$(","@выражение@",")","$)", "@часть выражения@"],
        "id": ["$id","id",  "@аргументы для id@", "@часть выражения@"],
        "const": ["$const","const", "@часть выражения@"],
        "-" : ["-","$-", "@выражение@"],
        "+" : ["+","$+", "@выражение@"]

    },
    "@часть выражения@": {
        "id" : ["@выражение@"],
        "const": ["@выражение@"],
        ">" : [">", "$>",  "@выражение@", "@часть выражения@"],
        "<" : ["<","$<",  "@выражение@", "@часть выражения@"],
        "=":["=","$==",   "@выражение@", "@часть выражения@"], 
        ">=":[">=","$>=", "@выражение@", "@часть выражения@"], 
        "<=":["<=","$<=", "@выражение@", "@часть выражения@"], 
        "#":["#","$!=", "@выражение@", "@часть выражения@"],
        "+" : ["+", "$+", "@выражение@", "@часть выражения@"],
        "-" : ["-", "$-", "@выражение@", "@часть выражения@"],
        "*" : ["*", "$*" , "@выражение@", "@часть выражения@"],
        "/" : ["/", "$/", "@выражение@", "@часть выражения@"],
        "MOD" : ["MOD", "$%", "@выражение@", "@часть выражения@"],
        "DIV" : ["DIV", "$/", "@выражение@", "@часть выражения@"],
        "~" : ["~", "$!", "@выражение@", "@часть выражения@"],
        "&" : ["&", "$&&", "@выражение@", "@часть выражения@"],
        "OR" : ["OR", "$||", "@выражение@", "@часть выражения@"],
        "EMPTY PRODUCTIONS": [";", ")","DO", "THEN", "OF"]
    }
}