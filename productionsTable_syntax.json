{
    "@программа@":{
        "MODULE": ["MODULE",   "id",   ";",   "@блок объявлений@", "BEGIN", "@тело@",
                "END", "id", "."]
    },
    "@блок объявлений@":{
        "VAR":["VAR",  "id",  "@посл. id@", ":", "@тип@",";","@объявление переменной@",   "@блок объявлений@"],
        "TYPE":["TYPE","id", "=", "@тип@",";", "@объявление типа@", "@блок объявлений@"],
        "PROCEDURE": ["PROCEDURE", "id", "(",  "@параметры процедуры@",")",
             "@возвращаемое значение процедуры@",  "@блок объявлений@","BEGIN",
             "@тело@","END","id",";"],
        "BEGIN": []
    },
    "@объявление переменной@": {
        "id": ["id", "@посл. id@", ":", "@тип@",";","@объявление переменной@"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE","PROCEDURE"]
    },
    "@посл. id@":{
        ",": [",",  "id",  "@посл. id@"],
        ":": []
    },
    "@объявление типа@": {
        "id": ["id", "=", "@тип@", ";"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE", "PROCEDURE"]
    },
    "@возвращаемое значение процедуры@": {
        ":": [":","@тип@"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE", "PROCEDURE"]
    },
    "@параметры процедуры@": {
        "id": ["id",  ":", "@тип@", "@остальные пар-ры проц-ры@"],
        ")": []
    },
    "@остальные пар-ры проц-ры@":{
        ",": [",",   "id", ":", "@тип@", "@остальные пар-ры проц-ры@"],
        ")": []
    },
    "@тип@": {
        "INTEGER" : ["INTEGER"],
        "REAL" : ["REAL"],
        "BOOLEAN" : ["BOOLEAN"],
        "CHAR" : ["CHAR"],
        "STRING" : ["STRING"],
        "ARRAY" : ["ARRAY", "const", "OF", "@тип@"]
    },
    "@тело@": {
        ";": [";",  "@тело@"],
        "const": ["@оператор@", "@тело@"],
        "id": ["@оператор@", "@тело@"],
        "IF": ["IF", "@выражение@", "THEN",  "@тело@", "@блоки elsif@", "@блок else@" ,"END", "@тело@"],
        "WHILE": ["WHILE",   "@выражение@", "DO", "@тело@","END", "@тело@"],
        "CASE": ["CASE",   "@выражение@", "OF", "@вариант@", "@варианты@", "@блок default@", "END",  "@тело@"],
        "REPEAT": ["REPEAT",    "@тело@", "UNTIL",  "@выражение@", "@тело@"],
        "DEC": ["DEC", "@значение@", ";", "@тело@"],
        "INC": ["INC", "@значение@", ";", "@тело@"],
        "RETURN": ["RETURN",  "@выражение@",  "@тело@"],
        "PRINT" : ["PRINT",  "(",  "@выражение@",  ")", "@тело@"],
        "READ" : ["READ","(",  "id", "@обращение к индексу массива@", ")",    "@тело@"],
        "EMPTY PRODUCTIONS" : ["END", "UNTIL", "ELSIF", "ELSE", "|"]
    },
    "@значение@" : {
        "id" : ["id"],
        "const" : ["const"]
    },
    "@блоки elsif@":{
        "ELSIF": ["ELSIF",   "@выражение@", "THEN",  "@тело@", "@блоки elsif@"],
        "EMPTY PRODUCTIONS": ["ELSE", "END"]
    },
    "@блок else@": {
        "ELSE": ["ELSE",  "@тело@"],
        "END" : []
    },
    "@блок default@": {
        "ELSE": ["ELSE",   "@тело@"],
        "END" : []
    },
    "@вариант@" : {
        "const": [  "const", ":", "@тело@"],
        "EMPTY PRODUCTIONS" : ["END"]
    },
    "@варианты@" : {
        "|": ["|", "@вариант@"],
        "EMPTY PRODUCTIONS" : ["END"]
    },
    "@оператор@": {
        "const": ["@пр. выражение@" ,";"],
        "id": ["id", "@действие с id@",";"]
    },
    "@действие с id@": {
        ":=": [":=",  "@выражение@"],
        "(": ["(", "@аргументы вызываемой функции@", ")", "@часть выражения@"],
        ";": []
    },
    "@аргументы вызываемой функции@": { 
        ")": [], 
        "id": ["@выражение@", "@аргументы вызываемой функции с запятой@"],
        "const" : ["@выражение@", "@аргументы вызываемой функции с запятой@"]
    },
    "@аргументы вызываемой функции с запятой@": {
        "," : [",", "@выражение@", "@аргументы вызываемой функции с запятой@"],
        ")" : []
    },
    "@дополнение к id@":
    {
        "(": ["@вызов функции@"],
        "[": ["@обращение к индексу массива@"],
        "EMPTY PRODUCTIONS": ["+","-", ">", "<", ">=", "#", "=", ";", "<=", "DIV", "MOD", "&", "OR", "*", "/", ")", "OF", "]"]
    },
    "@вызов функции@":{
        "(": ["(", "@аргументы вызываемой функции@",")"],
        "EMPTY PRODUCTIONS": ["+","-", ">", "<", ">=", "#", "=", ";", "<=", "DIV", "MOD", "&", "OR", "*", "/", ")", "OF", "]"]
    },
    "@обращение к индексу массива@":{
        "[": ["[",  "@выражение@", "]",  "@обращение к индексу массива@"],
        "EMPTY PRODUCTIONS": ["+","-", ">", "<", ">=", "#", "=", ";", "<=", "DIV", "MOD", "&", "OR", "*", "/", ")", "OF", "]"]
    },
    "@выражение@": {
        "(": ["(", "@выражение@",")", "@часть выражения@"],
        "id": ["id",  "@дополнение к id@", "@часть выражения@"],
        "const": ["const", "@часть выражения@"],
        "-" : ["-", "@выражение@"],
        "+" : ["+", "@выражение@"]

    },
    "@часть выражения@": {
        "id" : ["@выражение@"],
        "const": ["@выражение@"],
        ">" : [">",   "@выражение@", "@часть выражения@"],
        "<" : ["<",  "@выражение@", "@часть выражения@"],
        "=":["=",   "@выражение@", "@часть выражения@"], 
        ">=":[">=", "@выражение@", "@часть выражения@"], 
        "<=":["<=", "@выражение@", "@часть выражения@"], 
        "#":["#", "@выражение@", "@часть выражения@"],
        "+" : ["+",  "@выражение@", "@часть выражения@"],
        "-" : ["-",  "@выражение@", "@часть выражения@"],
        "*" : ["*",  "@часть выражения@"],
        "/" : ["/",  "@выражение@", "@часть выражения@"],
        "MOD" : ["MOD",  "@выражение@", "@часть выражения@"],
        "DIV" : ["DIV",  "@выражение@", "@часть выражения@"],
        "~" : ["~",  "@выражение@", "@часть выражения@"],
        "&" : ["&",  "@выражение@", "@часть выражения@"],
        "OR" : ["OR",  "@выражение@", "@часть выражения@"],
        "EMPTY PRODUCTIONS": [";", ")","DO", "THEN", "OF", "]"]
    }
}