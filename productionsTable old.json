{
    "@программа@":{
        "MODULE": ["MODULE", "id", ";", "@блок объявлений@", "BEGIN", "@тело@",
                "END", "id", "."]
    },
    "@блок объявлений@":{
        "VAR":["VAR", "id","@посл. id@", ":", "@тип@",";","@объявление переменной@", "@блок объявлений@"],
        "TYPE":["TYPE","id", "=", "@тип@",";", "@объявление типа@", "@блок объявлений@"],
        "PROCEDURE": ["PROCEDURE","id","(", "@параметры процедуры@",")",
             "@возвращаемое значение процедуры@", "@блок объявлений@","BEGIN",
             "@тело@","END","id",";"],
        "BEGIN": []
    },
    "@объявление переменной@": {
        "id": ["id","@посл. id@", ":", "@тип@",";","@объявление переменной@"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE","PROCEDURE"]
    },
    "@посл. id@":{
        ",": [",", "id", "@посл. id@"],
        ":": []
    },
    "@объявление типа@": {
        "id": ["id", "=", "@тип@", ";"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE", "PROCEDURE"]
    },
    "@возвращаемое значение процедуры@": {
        ":": [":","@тип@"],
        "EMPTY PRODUCTIONS": ["BEGIN", "VAR", "TYPE", "PROCEDURE"]
    },
    "@параметры процедуры@": {
        "id": ["id", ":", "@тип@", "@остальные пар-ры проц-ры@"],
        ")": []
    },
    "@остальные пар-ры проц-ры@":{
        ",": [",", "id", ":", "@тип@", "@остальные пар-ры проц-ры@"],
        ")": []
    },
    "@тип@": {
        "INTEGER" : ["INTEGER"],
        "REAL" : ["REAL"],
        "BOOLEAN" : ["BOOLEAN"],
        "CHAR" : ["CHAR"],
        "STRING" : ["STRING"],
        "ARRAY" : ["ARRAY", "OF", "@тип@"]
    },
    "@тело@": {
        ";": [";", "@тело@"],
        "const": ["@оператор@", "@тело@"],
        "id": ["@оператор@", "@тело@"],
        "IF": ["IF", "@выражение@", "THEN", "@тело@", "@блоки elsif@", "@блок else@" ,"END", "@тело@"],
        "WHILE": ["WHILE", "@выражение@", "DO", "@тело@","END", "@тело@"],
        "CASE": ["CASE", "@выражение@", "OF", "@вариант@", "@варианты@", "@блок else@", "END", "@тело@"],
        "REPEAT": ["REPEAT", "@тело@", "UNTIL", "@выражение@", "@тело@"],
        "END": [],
        "DEC": ["DEC", "@значение@", ";"],
        "INC": ["INC", "@значение@", ";"]
    },
    "@оператор@": {
        "const": ["@пр. выражение@", ";"],
        "id": ["id","@действие с id@",";"]
    },
    "@действие с id@": {
        ":=": [":=", "@выражение@"],
        "(": ["(","@аргументы вызываемой функции@", ")", "@последовательность слагаемых@"],
        ";": []
    },
    "@аргументы вызываемой функции@": { 
        ")": [], 
        "id": ["@выражение@", "@аргументы вызываемой функции с запятой@"],
        "const" : ["@выражение@", "@аргументы вызываемой функции с запятой@"]
    },
    "@аргументы вызываемой функции с запятой@": {
        "," : [",", "@выражение@", "@аргументы вызываемой функции с запятой@"],
        ")" : []
    },
    "@значение@" : {
        "id" : ["id"],
        "const" : ["const"]
    },
    "@выражение@": {
        "(": ["(","@выражение@",")"],
        "id": ["@пр. выражение@", "@сравнение с пр. выражением@"],
        "const": ["@пр. выражение@", "@сравнение с пр. выражением@"]
    },
    "@сравнение с пр. выражением@": { 
        ">" :[">",  "@пр. выражение@"], 
        "<" :["<",  "@пр. выражение@"], 
        "=":["=",   "@пр. выражение@"], 
        ">=":[">=", "@пр. выражение@"], 
        "<=":["<=", "@пр. выражение@"], 
        "#":["#", "@пр. выражение@"],
        "EMPTY PRODUCTIONS": [";", ")", ","] 
    }, 
    "@пр. выражение@": { 
        "+": ["@последовательность слагаемых@"], 
        "-": ["@последовательность слагаемых@"], 
        "(": ["(","@пр. выражение@",")"], 
        "id": ["@слагаемое@", "@последовательность слагаемых@"],
        "const" : ["@слагаемое@", "@последовательность слагаемых@"] 
    }, 
    "@последовательность слагаемых@": { 
        "+": ["+", "@слагаемое@", "@последовательность слагаемых@"], 
        "-": ["-", "@слагаемое@", "@последовательность слагаемых@"], 
        "OR": ["OR", "@слагаемое@", "@последовательность слагаемых@"],
        "EMPTY PRODUCTIONS": [",",")", ";", "<", ">","=", "#", ">=", "<=", "DO", "THEN"]
    }, 
    "@слагаемое@": { 
        "(": ["(","@пр. выражение@",")", "@последовательность множителей@"], 
        "id": ["@множитель@", "@последовательность множителей@"],
        "const": ["@множитель@", "@последовательность множителей@"] 
    }, 
    "@последовательность множителей@": { 
        "*": ["*", "@выражение@", "@последовательность множителей@"], 
        "/": ["/", "@выражение@", "@последовательность множителей@"], 
        "DIV": ["DIV", "@выражение@", "@последовательность множителей@"], 
        "MOD": ["MOD", "@выражение@", "@последовательность множителей@"], 
        "&": ["&", "@выражение@", "@последовательность множителей@"],
        "EMPTY PRODUCTIONS": ["+", "-", "OR", ";", ")", "<", ">","=", "#", ">=", "<=", "DO", "THEN", ","]
    }, 
    "@множитель@": { 
        "~": ["~","@выражение@"], 
        "id": ["id", "@аргументы для id@"],
        "const": ["const"]
    },
    "@аргументы для id@":{
        "(": ["(","@аргументы вызываемой функции@",")"],
        "EMPTY PRODUCTIONS": ["+","-", ">", "<", ">=", "#", ";", "<=", "DIV", "MOD", "&", "OR", "*", "/", ")"]
    }
}